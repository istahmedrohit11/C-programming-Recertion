#include <stdio.h>
int findHCF(int a, int b) {
    if (b == 0)
        return a;
    return findHCF(b, a % b);
}
int main() {
    int num1, num2;
    printf("Enter first number: ");
    scanf("%d", &num1);
    printf("Enter second number: ");
    scanf("%d", &num2);
    printf("HCF of %d and %d is: %d\n", num1, num2, findHCF(num1, num2));
    return 0;
}
